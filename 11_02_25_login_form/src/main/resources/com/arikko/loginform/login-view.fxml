<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.arikko.loginform.LoginViewController">
   <children>
      <Label layoutX="264.0" layoutY="72.0" style="-fx-font-weight: 700;" text="Login">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="163.0" layoutY="110.0" text="Welcome back! Let's take you to your account." />
      <Label layoutX="161.0" layoutY="154.0" text="Username" />
      <TextField fx:id="usernameTextField" layoutX="161.0" layoutY="175.0" prefHeight="26.0" prefWidth="253.0" />
      <Label layoutX="161.0" layoutY="214.0" text="Password" />
      <PasswordField fx:id="passwordPasswordField" layoutX="161.0" layoutY="236.0" onKeyTyped="#passwordOnKeyTyped" prefHeight="26.0" prefWidth="253.0" />
      <Label layoutX="314.0" layoutY="214.0" text="Forgot password?" />
      <Button layoutX="162.0" layoutY="280.0" mnemonicParsing="false" onAction="#loginOnAction" prefHeight="26.0" prefWidth="253.0" text="Login" />
      <Label layoutX="201.0" layoutY="312.0" text="Don't have an account? Sign up" />
   </children>
</AnchorPane>
